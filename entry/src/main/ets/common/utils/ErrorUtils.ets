export class ErrorUtils {  /**   * 数据库未打开   */  public static DATABASE_NOT_OPENED = ErrorUtils.toError(1001, $r("app.string.database_not_opened"));  /**   * 数据库未设置主密码   */  public static DATABASE_NOT_SET  = ErrorUtils.toError(1002, $r("app.string.password_not_set"));  /**   * 用户取消认证   */  public static AUTH_CANCELED  = ErrorUtils.toError(1002, $r("app.string.auth_canceled"));  /**   * 构建异常类   * @param code 编码   * @returns 结果   */  public static toError(code: number, message: ResourceStr): BusinessError {    return { code: code, message: message } as BusinessError;  }}